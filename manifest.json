{
    "manifest_version": 2,
    "name": "Synchro",
    "version": "1.0",
    "description": "Simple Kanban style productivity tracker chrome extension",
    "icons": { "128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "Synchro.html"
    },
    "permissions": [
        "storage"
    ]
}