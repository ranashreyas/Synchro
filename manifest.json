{
    "manifest_version": 2,
    "name": "Synchro",
    "version": "1.1",
    "description": "Simple and easy to use task tracker",
    "icons": { "128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "Synchro.html"
    },
    "permissions": [
        "storage"
    ]
}